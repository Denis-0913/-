    <connection>avia</connection>
    <sql>SELECT 
f.flight_id::int4,
f.flight_no,
f.scheduled_departure::timestamp,
f.scheduled_arrival::timestamp,
f.actual_departure::timestamp,
f.actual_arrival::timestamp,
f.departure_airport,
f.arrival_airport,
f.aircraft_code,
tf.fare_conditions,
tf.amount,
t.passenger_id 
FROM bookings.flights f
LEFT JOIN bookings.ticket_flights tf ON tf.flight_id = f.flight_id
LEFT JOIN bookings.tickets t ON t.ticket_no = tf.ticket_no 
Where status  = 'Arrived'</sql>
